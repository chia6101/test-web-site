runtime: nodejs14

instance_class: F1

service: backend # GAEのサービスの名前。frontendとかでも良いかも

env_variables:
  HOST: '0.0.0.0'
  NODE_ENV: 'production'
  DATABASE_NAME: ${{secrets.DB_NAME}}
  DATABASE_USERNAME: ${{secrets.DB_USER}}
  DATABASE_PASSWORD: ${{secrets.DB_PASSWORD}}
  INSTANCE_CONNECTION_NAME: ${{secrets.DB_CONNECTION_NAME}}
  
beta_settings:
  cloud_sql_instances: ${{secrets.DB_CONNECTION_NAME}}
